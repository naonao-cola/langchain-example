    "#导入需要的依赖包\n",
    "from langchain_community.llms import Ollama\n",
    "import langchain\n",
    "from langchain.globals import set_debug\n",
    "#配置模型信息\n",
    "llm = Ollama(model=\"qwen:1.8b\")\n",
    "langchain.debug = True\n",
    "# set_debug(True)\n",
    "\n",
    "#调用大语言模型的预测功能\n",
    "llm.invoke(\"你是谁？\")"
